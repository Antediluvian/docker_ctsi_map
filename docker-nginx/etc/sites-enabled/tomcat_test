upstream gis {
    server tomcat_test:8080;
}

server {
    listen 80;
    server_name localhost;

    access_log /home/nginx/var/logs/tests_gis_access.log;
    error_log /home/nginx/var/logs/tests_gis_error.log;

    location / {
        proxy_pass        http://gis;

        proxy_set_header  Host		$host;
        proxy_set_header  X-Real-IP	$remote_addr;
        proxy_set_header  X-Forwarded-For	$proxy_add_x_forwarded_for;
    }
}

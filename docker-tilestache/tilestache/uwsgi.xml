<uwsgi>
    <for>8001 8002 8003 8004</for>
        <socket>0.0.0.0:%(_)</socket>
    <endfor/>

    <gid>www-data</gid>
    <uid>uwsgi</uid>

    <master/>
    <thunder-lock/>
    <enable-threads/>
    <memory-report/>
    <processes>8</processes>
    <stats>/tmp/stats.socket</stats>

    <wsgi-file>/home/tilestache/tilestache.wsgi</wsgi-file>
</uwsgi>

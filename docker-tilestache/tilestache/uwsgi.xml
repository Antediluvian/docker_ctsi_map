<uwsgi>
    <for>8001 8002 8003 8004</for>
        <socket>0.0.0.0:%(_)</socket>
    <endfor/>

    <virtualenv>.ve</virtualenv>
    <gid>www-data</gid>
    <uid>uwsgi</uid>

    <pythonpath>/home/python/.ve/lib/python2.7</pythonpath>

    <master/>
    <thunder-lock/>
    <enable-threads/>
    <memory-report/>
    <processes>8</processes>
    <stats>/tmp/stats.socket</stats>

    <pidfile>/home/python/var/run/uwsgi.pid</pidfile>
    <wsgi-file>/mnt/tilestache/tilestache.wsgi</wsgi-file>
</uwsgi>
